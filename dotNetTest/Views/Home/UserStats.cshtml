@using dotNetTest.Models

@{
    /**/

    ViewBag.Title = "Home Page";

    Model stats = new Model();

    stats.UserStats(out List<string> questions, out List<string> count);
}
@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Chart Title")
        .AddSeries(
            name: "Employee",
            xValue: questions,
            yValues: count).Write();
}